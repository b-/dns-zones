{
  "bind": {
    "TYPE": "BIND"
  },
  "cloudflare": {
    "TYPE": "CLOUDFLAREAPI",
    "accountid": "$CLOUDFLARE_ACCOUNTID",
    "apitoken": "$CLOUDFLARE_APITOKEN"
  },
  "axfrddns": {
    "TYPE": "AXFRDDNS",
    "nameservers": "tdns1.ibeep.com,tdns2.ibeep.com",
    "master": "tdns2.ibeep.com",
    "transfer-key": "$TSIG_CI",
    "update-key": "$TSIG_CI"
  },

  "tdns1": {
    "TYPE": "AXFRDDNS",
    "nameservers": "tdns1.ibeep.com,tdns2.ibeep.com",
    "master": "tdns1.shark-perch.ts.net",
    "transfer-key": "$TSIG_CI",
    "update-key": "$TSIG_CI"
  },

  "tdns2": {
    "TYPE": "AXFRDDNS",
    "nameservers": "tdns1.ibeep.com,tdns2.ibeep.com",
    "master": "tdns2.ibeep.com",
    "transfer-key": "$TSIG_CI",
    "update-key": "$TSIG_CI"
  }
}
